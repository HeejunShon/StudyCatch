<!-- 삭제할 때 교육자료와 챕터 중 어떤 것을 삭제할 것인지 생각해보기 -->
<div class="ph-float">
    <form action="/community/<%= communityID %>/study_delete" method="post"
        onsubmit="return confirm('교육자료를 다 날리실건가요?')">
        <input type="hidden" name="classID" value="<%= classID %>">
        <input type="hidden" name="chapterID" value="<%= chapterID %>">
        <% filedata.forEach(function (item, index, array){ %>
            <input type="hidden" name="fileId" value="<%= array[index].fid %>">
        <% }); %>
        <input type="submit" value="교육자료삭제" class="ph-button ph-btn-deepgreen">
    </form>
</div>
<div class="ph-float">
    <form action="/community/<%= communityID %>/chapter_delete" method="post"
        onsubmit="return confirm('정말 이 챕터를 몽땅 없애버릴 건가요?')">
        <input type="hidden" name="classID" value="<%= classID %>">
        <input type="hidden" name="chapterID" value="<%= chapterID %>">
        <% filedata.forEach(function (item, index, array){ %>
            <input type="hidden" name="fileId" value="<%= array[index].fid %>">
        <% }); %>
        <input type="submit" value="챕터삭제" class="ph-button ph-btn-deepgreen">
    </form>
</div>